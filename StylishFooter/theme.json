{
  "name": "Stylish Footer",
  "version": "v1.1.0",
  "author": "Tormak",
  "target": "System-Wide",
  "description": "This theme adds flare to the footer!",
  "manifest_version": 6,
  "inject": {
    "shared.css": [ "QuickAccess", "SP", "MainMenu" ]
  },
  "patches": {
    "Switch Like Home Fix": {
      "type": "toggle",
      "default": "No",
      "values": {
        "Yes": {
          "compat/noFriendsSwitch.css": [ "QuickAccess", "SP", "MainMenu" ]
        },
        "No": {}
      }
    },
    "Clean Game View Fix": {
      "type": "toggle",
      "default": "No",
      "values": {
        "Yes": {
          "compat/cleanGameView.css": [ "QuickAccess", "SP", "MainMenu" ]
        },
        "No": {}
      }
    },
    "Minimal Lock Screen Fix": {
      "type": "toggle",
      "default": "No",
      "values": {
        "Yes": {
          "compat/minimalLockScreen.css": [ "QuickAccess", "SP", "MainMenu" ]
        },
        "No": {}
      }
    }
  }
}